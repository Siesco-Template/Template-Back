      "UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "afmis-main-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/afmis/{everything}",
      "UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
